    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-livereload": "2.0.5",
    "rollup-plugin-polyfill-node": "0.13.0",
    "rollup-plugin-svelte": "^7.2.0",
    "svelte": "4.2.17",
    "svelte-check": "^3.8.0",
    "svelte-preprocess": "5.1.4",
